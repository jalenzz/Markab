{
    "name": "markab",
    "private": true,
    "version": "0.6.0",
    "type": "module",
    "description": "Bookmark New Tab, Never forget your bookmarks",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "build:analyze": "tsc -b && ANALYZE=true vite build",
        "preview": "vite preview",
        "lint": "eslint . && stylelint \"src/**/*.{css,scss,less}\"",
        "lint:fix": "eslint . --fix && stylelint \"src/**/*.{css,scss,less}\" --fix && prettier --write .",
        "format": "prettier --write .",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@emoji-mart/react": "^1.1.1",
        "immer": "^10.1.1",
        "motion": "^12.17.0",
        "pinyin-pro": "^3.26.0",
        "react": "^19.1.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^19.1.0"
    },
    "devDependencies": {
        "@crxjs/vite-plugin": "^2.0.0",
        "@eslint/js": "^9.25.0",
        "@types/chrome": "^0.0.326",
        "@types/node": "^22.13.14",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.4.1",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.25.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^16.0.0",
        "postcss": "^8.5.5",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.12",
        "rollup-plugin-visualizer": "^6.0.3",
        "stylelint": "^16.20.0",
        "stylelint-config-tailwindcss": "^1.0.0",
        "stylelint-order": "^7.0.0",
        "tailwindcss": "3.4.17",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^6.3.5",
        "vite-plugin-zip-pack": "^1.2.4"
    }
